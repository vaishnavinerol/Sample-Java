pipeline {
    agent any


    stages {
        stage("Stage 1") {
            steps {
                script {

                    env.BAR = "welcome"
					echo "${BAR}"
                }
            }
        }

        stage("Stage 2") {
            steps {
                script {
                    echo "${BAR}"
                    env.BAR = "bar2345"
					echo "${BAR}"
					sh '''
					ABC="wxs"
					DJSD=echo"${ABC}"
					
					'''
					
					
                }
            }
        }

        
    }
}
